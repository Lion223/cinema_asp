@model IEnumerable<CinemaASP.FGAs>

@{
    ViewBag.Title = "FGAs";
}

<h2>FGAs</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Film_Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Genre_Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Actor_Id)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Film_Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Genre_Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Actor_Id)
        </td>
    </tr>
}

</table>

<p>
    @Html.ActionLink("Main menu", "..")
</p>